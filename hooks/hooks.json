[
  {
    "id": "li-update",
    "execute-command": "/usr/bin/git",
    "command-working-directory": "~/li",
    "response-message": "li pulled successfully.",
    "pass-arguments-to-command": [
      { "source": "string", "name": "pull" },
      { "source": "string", "name": "origin" },
      { "source": "string", "name": "main" }
    ]
  },
  {
    "id": "ytdlp-update",
    "execute-command": "sh ~/li/hooks/ytdlp-update.sh",
    "command-working-directory": "~/li/hooks",
    "response-message": "ytdlp pulled successfully."
  }
]
